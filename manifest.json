{
  "manifest_version": 2,
  "name": "KW Highlighter",
  "description": "Highlights the current KW",
  "homepage_url": "https://github.com/libf-de/firefox-kw-highlight",
  "version": "1.0",
  "icons": {
    "48": "icons/icon.png",
    "96": "icons/icon@2x.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./substitute.js"]
    }
  ]
}
